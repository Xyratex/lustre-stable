				</test_values>
			</example>
			</examples>
				<tags>
					<tag>ldiskfs_warning</tag>
					<tag>iem_reinject</tag>
				</tags>
				<actions>
					<action>
						<values>
							<value name="MESSAGE">IEC: 026001004: LDISKFS WARNING: { "device": "$device", "warn_msg": "$warn_msg" }</value>
						</values>
