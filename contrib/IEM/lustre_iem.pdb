			<rule provider="lustre" id="ce447a46-7b29-4c87-a955-4adb84b42e52" class="system">
				<patterns>
					<pattern>LNetError: @NUMBER:pid@:@NUMBER:ext_pid@:(@ESTRING:file::@@NUMBER:line@:@ESTRING:func:(@)) ASSERTION@ANYSTRING:assert_info@</pattern>
